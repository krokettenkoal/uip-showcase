{
	"name": "uas-showcase",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run generate && vite build",
		"preview": "vite preview",
		"cc-startup": "npm ci && node index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate": "npm run theme && npm run api",
		"theme": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/css/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/css/smui-dark.css -i src/theme/dark",
		"api": "openapi-generator-cli generate --generator-key showcase"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@fortawesome/free-brands-svg-icons": "^6.6.0",
		"@material/dom": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@openapitools/openapi-generator-cli": "^2.14.0",
		"@smui-extra/accordion": "^7.0.0",
		"@smui-extra/autocomplete": "^7.0.0",
		"@smui-extra/badge": "^7.0.0",
		"@smui/button": "^7.0.0",
		"@smui/card": "^7.0.0",
		"@smui/checkbox": "^7.0.0",
		"@smui/chips": "^7.0.0",
		"@smui/circular-progress": "^7.0.0",
		"@smui/common": "^7.0.0",
		"@smui/data-table": "^7.0.0",
		"@smui/dialog": "^7.0.0",
		"@smui/drawer": "^7.0.0",
		"@smui/fab": "^7.0.0",
		"@smui/form-field": "^7.0.0",
		"@smui/icon-button": "^7.0.0",
		"@smui/linear-progress": "^7.0.0",
		"@smui/list": "^7.0.0",
		"@smui/paper": "^7.0.0",
		"@smui/segmented-button": "^7.0.0",
		"@smui/select": "^7.0.0",
		"@smui/snackbar": "^7.0.0",
		"@smui/switch": "^7.0.0",
		"@smui/tab": "^7.0.0",
		"@smui/tab-bar": "^7.0.0",
		"@smui/textfield": "^7.0.0",
		"@smui/top-app-bar": "^7.0.0",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-node": "^5.2.6",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.7.0",
		"@types/node": "^22.7.5",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.1",
		"globals": "^15.11.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"smui-theme": "^7.0.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"svelte-fa": "^4.0.3",
		"svelte-highlight": "^7.7.0",
		"tslib": "^2.7.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.8.1",
		"vite": "5.4.8"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "0.37.0",
		"@auth/sveltekit": "^1.7.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"mysql2": "3.11.3"
	}
}
