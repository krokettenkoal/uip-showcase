{
	"name": "uas-showcase",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run generate && vite build",
		"preview": "vite preview",
		"cc-startup": "npm ci --omit dev && node index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "npm run theme && npm run api",
		"theme": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/css/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/css/smui-dark.css -i src/theme/dark",
		"api": "openapi-generator-cli generate --generator-key showcase"
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@material/dom": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@openapitools/openapi-generator-cli": "^2.14.0",
		"@smui-extra/accordion": "^7.0.0-beta.15",
		"@smui-extra/badge": "^7.0.0-beta.15",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/card": "^7.0.0-beta.15",
		"@smui/chips": "^7.0.0-beta.15",
		"@smui/circular-progress": "^7.0.0-beta.15",
		"@smui/common": "^7.0.0-beta.15",
		"@smui/drawer": "^7.0.0-beta.15",
		"@smui/fab": "^7.0.0-beta.15",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/linear-progress": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.15",
		"@smui/paper": "^7.0.0-beta.15",
		"@smui/segmented-button": "^7.0.0-beta.15",
		"@smui/tab": "^7.0.0-beta.15",
		"@smui/tab-bar": "^7.0.0-beta.15",
		"@smui/textfield": "^7.0.0-beta.15",
		"@smui/top-app-bar": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@types/node": "^20.8.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-fa": "^3.0.4",
		"svelte-highlight": "^7.3.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.5.3"
	},
	"type": "module",
	"dependencies": {
		"mysql2": "^3.9.7"
	}
}
